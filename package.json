{
  "name": "imogi-pos-react",
  "version": "2.0.0",
  "description": "React frontend for IMOGI POS - Centralized multi-app architecture with 12 apps",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "VITE_APP=cashier-console vite",
    "dev:cashier": "VITE_APP=cashier-console vite",
    "dev:kitchen": "VITE_APP=kitchen vite",
    "dev:waiter": "VITE_APP=waiter vite",
    "dev:kiosk": "VITE_APP=kiosk vite",
    "dev:self-order": "VITE_APP=self-order vite",
    "dev:customer-display": "VITE_APP=customer-display vite",
    "dev:table-display": "VITE_APP=table-display vite",
    "dev:customer-display-editor": "VITE_APP=customer-display-editor vite",
    "dev:table-management-editor": "VITE_APP=table-management-editor vite",
    "dev:module-select": "VITE_APP=module-select vite",
    "build": "npm run build:all",
    "build:all": "npm run build:cashier && npm run build:kitchen && npm run build:waiter && npm run build:kiosk && npm run build:self-order && npm run build:customer-display && npm run build:table-display && npm run build:customer-display-editor && npm run build:table-management-editor && npm run build:module-select",
    "build:debug": "npm run build:cashier:debug && npm run build:kitchen:debug && npm run build:waiter:debug && npm run build:kiosk:debug && npm run build:module-select:debug",
    "build:cashier": "VITE_APP=cashier-console vite build",
    "build:cashier:debug": "VITE_APP=cashier-console VITE_DEBUG=true vite build",
    "build:kitchen": "VITE_APP=kitchen vite build",
    "build:kitchen:debug": "VITE_APP=kitchen VITE_DEBUG=true vite build",
    "build:waiter": "VITE_APP=waiter vite build",
    "build:waiter:debug": "VITE_APP=waiter VITE_DEBUG=true vite build",
    "build:kiosk": "VITE_APP=kiosk vite build",
    "build:kiosk:debug": "VITE_APP=kiosk VITE_DEBUG=true vite build",
    "build:self-order": "VITE_APP=self-order vite build",
    "build:customer-display": "VITE_APP=customer-display vite build",
    "build:table-display": "VITE_APP=table-display vite build",
    "build:customer-display-editor": "VITE_APP=customer-display-editor vite build",
    "build:table-management-editor": "VITE_APP=table-management-editor vite build",
    "build:module-select": "VITE_APP=module-select vite build",
    "build:module-select:debug": "VITE_APP=module-select VITE_DEBUG=true vite build",
    "preview": "vite preview",
    "check:api-constants": "node scripts/check-api-constants.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "frappe-react-sdk": "^1.14.0",
    "react": "^18.3.1",
    "react-color": "^2.19.3",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^2.2.2",
    "react-hook-form": "^7.71.1",
    "reactflow": "^11.11.4",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "madge": "^8.0.0",
    "vite": "^5.4.11"
  }
}
